  "exports": {
    ".": {
      "types": "./dist/vue-components.es.d.ts",
      "import": "./dist/vue-components.es.js",
      "require": "./dist/vue-components.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
